<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.seonhui.app.member.MemberMapper">
  	<insert id="memberJoin" parameterType="MemberVO">
  		INSERT INTO MEMBERS VALUES(#{memberId}, #{memberPassword}, #{memberName}, #{memberNickname}, #{memberMobile}, #{memberEmail}, #{memberBirthday})
  	</insert>
  	
  	<select id="findMember" parameterType="MemberVO">
  		SELECT * FROM MEMBERS WHERE MEMBER_ID=#{memberID}
  	</select>
  	
  	
  </mapper>